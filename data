(setq start1 '((make-main START1TEMPL)))
(setq START1TEMPL '(start1 (starter) (durer) (pitcher) 20 1.0 15 .07 7.00 0 1 1 7))
(setq fret1 '((make-main FRET1TEMPL)))
(setq FRET1TEMPL '(fret1 (starter) (durer) (pitcher) 20 1.0 15 .07 7.00 0 1))
(setq bend1 '((make-pre `BEND) (make-main BEND1TEMPL)))
(setq BEND1TEMPL '(bend1 (starter) (durer) (pitcher) (+ PITCH .02) 1 20 1.0 15 .07 7.00 0 1))
(setq vstart1 '((make-pre `VIB) (make-main VSTART1TEMPL)))
(setq VSTART1TEMPL '(vstart1 (starter) (durer) (pitcher) 20 1.0 15 .07 7.00 0 1 1 7 5 7 (vibdepth) (frandom 1)))
(setq vfret1 '((make-pre `VIB) (make-main VFRET1TEMPL)))
(setq VFRET1TEMPL '(vfret1 (starter) (durer) (pitcher) 20 1.0 15 .07 7.00 0 1 5 7 (vibdepth) (frandom 1)))
(setq vbend1 '((make-pre `VBEND) (make-main VBEND1TEMPL)))
(setq VBEND1TEMPL '(vbend1 (starter) (durer) (pitcher) (+ PITCH .02) 3 20 1.0 15 .07 7.00 0 1 5 7 (vibdepth) (frandom 1)))
(setq gliss '((make-pre `GLISS) (make-main GLISSTEMPL)))
(setq GLISSTEMPL '(gliss (starter) (durer) (pitcher) 20 1.0 15 .07 7.00 0 1))
(setq vgliss '((make-pre `VGLISS) (make-main VGLISSTEMPL)))
